    "import nltk  \n",
    "\n",
    "from bs4 import BeautifulSoup\n",
    "from requests import get\n",
    "\n",
    "#url = 'https://www.imdb.com/list/ls095374765/?sort=list_order,asc&st_dt=&mode=grid&page=1&ref_=ttls_ref_gnr&genres=Drama'\n",
